{
    "manifestVersion": 1,
    "id": "send-change-event",
    "name": "PagerDuty Send Change Event",
    "version": "1.0.3",
    "publisher": "KnightonDev",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Uses the PagerDuty Events API to send change messages to a service.",
    "categories": [
        "Azure Pipelines"
    ],
    "tags": [
        "PagerDuty",
        "Change Events"
    ],
    "galleryFlags": [
        "Public"
    ],
    "branding": {
        "color": "rgb(1, 58, 29)",
        "theme": "dark"
    },   
    "icons": {
        "default": "images/extension-icon.png"        
    },
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "LICENSE.md"
        }
    },  
    "files": [
        {
            "path": "sendchangeevent"
        }
    ],
    "links": {     
        "support": {
            "uri": "https://github.com/Knighton-Dev/sturdy-octo-disco/issues"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/Knighton-Dev/sturdy-octo-disco"
    }, 
    "contributions": [
        {
            "id": "custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "sendchangeevent"
            }
        }
    ]
}